<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<fx:root alignment="CENTER" prefHeight="500.0" prefWidth="300.0" spacing="50.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@stylesheet.css" />
    </stylesheets>
   <children>
      <Label text="Hoeveel slagen heeft elke speler gewonnen?" >
          <font>
              <Font name="Palatino Linotype" size="14.0" />
          </font>
      </Label>
      <VBox fx:id="vbResultBoxes" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" />
      <Button fx:id="btnOK" mnemonicParsing="false" onAction="#ok" text="OK" />
   </children>
</fx:root>
