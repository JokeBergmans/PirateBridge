<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" prefHeight="500.0" prefWidth="300.0" spacing="20.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Scores">
         <font>
            <Font name="Palatino Linotype" size="14.0" />
         </font></Label>
      <Label fx:id="lblRound">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <GridPane fx:id="grid" alignment="CENTER" gridLinesVisible="true">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </VBox.margin>
      </GridPane>
      <Button fx:id="btnNext" mnemonicParsing="false" onAction="#nextRound" text="Volgende Ronde">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
    <stylesheets>
        <URL value="@stylesheet.css" />
    </stylesheets>
</fx:root>
